footer.relative
    .container
        nav.md-flex
            .content.col-md-3
               h2.site-name Nanum.
               p 2453 Koontz Lane, North Hollywood, CA, United States
            -
                var navs = ['Company', 'Services', 'Useful links'],
                    links = ['Home', 'services', 'About', 'Blog', 'Contact']
            each nav in navs
                .content.col-md-3
                    h2.title #{nav}
                    ul
                        each link in links
                           li #[a(href='#') #{link}]
    .container.copyright.md-flex
        .col-md-3
            span 2018 &copy; Nanum.

        nav.col-md-6
            .flex
                -
                    var links = ['Home', 'services', 'About', 'Blog', 'Contact']
                each link in links
                    a(href='#') #{link}

        .socials.col-md-3
            -
                var arr = [
                {name:'google-plus', class:'fab fa-google-plus-g'},
                {name:'twitter', class:'fab fa-twitter'},
                {name:'instagram', class: 'fab fa-instagram'},
                {name:'linkedin', class: 'fab fa-linkedin-in'}
                ]
            each social in arr
                a(class=social.class href="#")

